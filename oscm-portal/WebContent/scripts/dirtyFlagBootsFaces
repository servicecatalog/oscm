var Document = {
	storedForm: []
};

Document.dirtyFlagSetOnMenu = function (id) {
        var element = document.getElementById(id);
      	$(element).select2().on('select2:selecting', function (e) {
        if (AdmUtils.isNotDirtyOrConfirmed()) {
          AdmUtils.storeValue(this);
        } else {
          e.preventDefault();
        }
      });
}
